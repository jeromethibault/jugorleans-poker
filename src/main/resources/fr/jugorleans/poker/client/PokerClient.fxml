          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <fx:include source="Table.fxml" fx:id="table" GridPane.rowIndex="0"/>

    <VBox GridPane.rowIndex="1" alignment="CENTER">
