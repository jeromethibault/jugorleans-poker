<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableView?>
<StackPane fx:controller="fr.jugorleans.poker.client.TournamentController"
          xmlns:fx="http://javafx.com/fxml" alignment="center">

    <fx:include source="Table.fxml" fx:id="table" />

    <VBox  alignment="CENTER" fx:id="tournamentPane">
        <TableView fx:id="tounamentList" prefHeight="1000" prefWidth="1000"/>
        <Button id="creerTournoi" text="New Tournament" onAction="#handleSubmitButtonAction"/>
    </VBox>

</StackPane>

